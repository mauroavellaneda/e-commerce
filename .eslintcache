item) => (\n                    <MenuItem key={item.id} value={item.id}>\n                      {item.label}\n                    </MenuItem>\n                  ))}\n              </Select>\n            </Grid>\n          </Grid>\n          <br />\n          <div style={{ display: \"flex\", justifyContent: \"space-between\" }}>\n            <Button component={Link} variant=\"outlined\" to=\"/cart\">\n              Back to Cart\n            </Button>\n            <Button type=\"submit\" variant=\"contained\" color=\"primary\">\n              Next\n            </Button>\n          </div>\n        </form>\n      </FormProvider>\n    </>\n  );\n};\n\nexport default AddressForm;\n","/Users/mauroavellaneda/e-commerce/src/components/CheckoutForm/PaymentForm.jsx",[],"/Users/mauroavellaneda/e-commerce/src/components/CheckoutForm/CustomTextField.jsx",["94","95"],"import React from \"react\";\nimport { Text, Grid, Container, TextField } from \"@material-ui/core\";\nimport { useFormContext, Controller } from \"react-hook-form\";\n\nconst FormInput = ({ name, label }) => {\n  const { control } = useFormContext();\n  return (\n    <Grid item xs={12} sm={6}>\n      <Controller\n        defaultValue=\"\"\n        as={TextField}\n        control={control}\n        fullWidth\n        name={name}\n        label={label}\n      />\n    </Grid>\n  );\n};\n\nexport default FormInput;\n","/Users/mauroavellaneda/e-commerce/src/components/CheckoutForm/Review.jsx",[],["96","97"],{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","severity":1,"message":"103","line":49,"column":6,"nodeType":"104","endLine":49,"endColumn":8,"suggestions":"105"},{"ruleId":"102","severity":1,"message":"106","line":62,"column":6,"nodeType":"104","endLine":62,"endColumn":27,"suggestions":"107"},{"ruleId":"108","severity":1,"message":"109","line":2,"column":10,"nodeType":"110","messageId":"111","endLine":2,"endColumn":14},{"ruleId":"108","severity":1,"message":"112","line":2,"column